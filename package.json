{
  "name": "@lotusengine/cli",
  "description": "LotusEngine CLI",
  "version": "0.0.0",
  "author": "@lotusengine",
  "bin": {
    "lotusengine": "./bin/run"
  },
  "bugs": "https://github.com/lotusengine/cli/issues",
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@oclif/command": "^1",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3",
    "apollo-cache-inmemory": "^1.1.0",
    "apollo-client": "^2.0.3",
    "apollo-link": "^1.0.3",
    "apollo-link-http": "^1.2.0",
    "aws-appsync": "^4.0.3",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "cross-fetch": "^3.1.4",
    "dotenv-flow": "^3.2.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.11.0",
    "kikstart-graphql-client": "^1.5.0",
    "netrc-parser": "^3.1.6",
    "open": "^8.0.5",
    "pluralize": "^8.0.0",
    "printf": "^0.6.1",
    "stringify-object": "^3.3.0",
    "tslib": "^2.2.0",
    "uuid": "^8.3.2",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@lotusengine/eslint-config-lotusengine": "https://github.com/lotusengine/eslint-config-lotusengine",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1.2.8",
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.1",
    "eslint": "^7.24.0",
    "globby": "^11.0.3",
    "jest": "^26.6.3",
    "oclif": "^1.17.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/lotusengine/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "helpClass": "./src/help",
    "commands": "./src/commands",
    "bin": "lotusengine",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "auth": {
        "description": "authenticate with CLI app"
      },
      "account": {
        "description": "manage user accounts and granted permissions"
      },
      "workflow": {
        "description": "manage service workflows"
      },
      "collection": {
        "description": "manage service collections"
      },
      "view": {
        "description": "manage service views"
      },
      "stack": {
        "description": "manage stacks"
      },
      "service": {
        "description": "manage services"
      },
      "module": {
        "description": "manage custom modules"
      },
      "user": {
        "description": "manage user access"
      }
    }
  },
  "repository": "@lotusengine/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "DISposttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "oclif": "oclif",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
